[alias]
    st = status
    ci = commit
    co = checkout
		rbpick = !BRANCH=`git symbolic-ref refs/remotes/origin/HEAD | sed 's@^refs/remotes/origin/@@'` && git fetch origin "$BRANCH" && ID=`git log --format="%H" -n 1` && git reset --hard "origin/$BRANCH" && git cherry-pick "$ID" && :

